            {
              "id": "CA2261",
              "shortDescription": {
                "text": "Do not use ConfigureAwaitOptions.SuppressThrowing with Task<TResult>"
              },
              "fullDescription": {
                "text": "The ConfigureAwaitOptions.SuppressThrowing option is only supported with the non-generic Task, not a Task<TResult>.  To use it with a Task<TResult>, first cast to the base Task."
              },
              "helpUri": "https://learn.microsoft.com/dotnet/fundamentals/code-analysis/quality-rules/ca2261",
