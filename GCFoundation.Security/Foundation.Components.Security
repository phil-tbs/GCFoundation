            {
              "id": "CA1514",
              "shortDescription": {
                "text": "Avoid redundant length argument"
              },
              "fullDescription": {
                "text": "An explicit length calculation can be error-prone and can be avoided when slicing to end of the buffer."
              },
              "defaultConfiguration": {
                "level": "note"
              },
              "helpUri": "https://learn.microsoft.com/dotnet/fundamentals/code-analysis/quality-rules/ca1514",
              "properties": {
